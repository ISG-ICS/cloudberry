{
  "dataset": "dataset.name",
  "lookup": [
    {
      "sourceKey": [
        "keyName"
      ],
      "dataset": "name",
      "lookupKey": [
        "keyName"
      ],
      "select": [
        "outputKeyName"
      ],
      "as": [
        "asName"
      ]
    }
  ],
  "filter": [
    {
      "field": "fieldP",
      "apply": "func|null",
      "relation": "==|<|>|<=|>=|isTrue(default)|in|contains|inRange",
      "values": [
        "x"
      ]
    }
  ],
  "group": {
    "by": [
      {
        "field": "fieldA",
        "apply": "function",
        "groups": 1000,
        "as": "outputString"
      }
    ],
    "summarize": [
      {
        "field": "fieldB|*",
        "apply": {
          "name": "topK",
          "args": {
            "k": 10
          }
        },
        "as": "outputFieldName"
      }
    ]
  },
  "order": {
    "on": [
      "fields"
    ],
    "limit": 100,
    "offset": 0,
    "select": [
      "fieldNames"
    ]
  }
}
