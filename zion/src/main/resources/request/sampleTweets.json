{
  "dataset" : "twitter.ds_tweet",
  "filter": [
    {
      "field" : "geo.stateID",
      "relation" : "in",
      "values" : [1,2,3,31]
    },
    {
      "field" : "create_at",
      "relation" : "inRange",
      "values" : ["2012", "2016"]
    },
    {
      "field" : "text",
      "relation" : "contains",
      "values" : ["zika", "virus"]
    }
  ],
  "select" : {
    "order" : [ "-create_at"],
    "limit": 100,
    "offset" : 0,
    "fields": ["id", "user", "create_at"]
  }
}