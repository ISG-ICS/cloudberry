{
  "dataset" : "twitter.ds_tweet",
  "filter": [
    {
      "field" : "geo_tag.stateID",
      "relation" : "in",
      "values" : [1,2,3,31]
    },
    {
      "field" : "create_at",
      "relation" : "inRange",
      "values" : ["2012", "2016"]
    },
    {
      "field" : "text",
      "relation" : "contains",
      "values" : ["zika", "virus"]
    }
  ],
  "group": {
    "by": [
      {
        "field": "hashTags",
        "apply": "unnest",
        "as" : "hashTag"
      }
    ],
    "aggregate": [
      {
        "field" : "hashTag",
        "apply" : "count",
        "as" : "count"
      }
    ]
  },
  "select" : {
    "order" : [ "-count"],
    "limit": 100,
    "offset" : 0
  }
}